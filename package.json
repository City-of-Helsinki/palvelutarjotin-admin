  "lint-staged": {
    "src/**/*.{tsx,ts}": [
      "eslint --fix",
      "yarn test:staged",
      "prettier --write"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(axios|react-helsinki-headless-cms|react-helsinki-headless-cms/apollo))/",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy",
      "axios": "<rootDir>/node_modules/axios/dist/node/axios.cjs"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__snapshots__/",
      "query.ts",
      "mutation.ts",
      "types.ts",
      "testUtils.tsx",
      "/icons/",
      "serviceWorker.ts",
      "./src/index.tsx",
      "datepickerContext.tsx",
      "mockDataUtils.ts",
      "ScrollToTop.tsx",
      "scrollToTop.ts",
      "App.tsx",
      "apolloClient.ts",
      "graphql.tsx",
      "OidcCallback.tsx",
      "SilentRenew.tsx"
    ]
