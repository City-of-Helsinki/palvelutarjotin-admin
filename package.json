  "scripts": {
    "app:version": "echo \"$npm_package_version\"",
    "prepare": "husky install",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "start-react-scripts": "cross-env SASS_PATH=$(cd ./src/styles && pwd) react-scripts start",
    "build-react-scripts": "cross-env SASS_PATH=$(cd ./src/styles && pwd) react-scripts build",
    "test-react-scripts": "cross-env SASS_PATH=$(cd ./src/styles && pwd) react-scripts test",
    "lint": "eslint --ext js,ts,tsx src",
    "test:changed": "yarn test -o --watchAll=false",
    "test:coverage": "yarn test --coverage --watchAll=false",
    "ci": "yarn test --verbose --coverage --runInBand",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:nowatch": "react-scripts test --watchAll=false",
    "test:staged": "yarn test:nowatch --findRelatedTests",
    "browser-test": "testcafe \"chrome --window-size='1920,1080'\" testcafe/e2e/",
    "browser-test:ci": "testcafe \"chrome:headless --disable-gpu --window-size='1920,1080'\" --screenshots takeOnFails=true,path=report,fullPage=true --video report testcafe/e2e/",
    "codegen": "DOTENV_CONFIG_PATH=\"./.env.development.local\" graphql-codegen -r dotenv/config --config codegen.yml",
    "release": "standard-version"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
