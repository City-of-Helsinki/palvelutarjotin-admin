  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint --ext js,ts,tsx src",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:changed": "npm test -- -o --watchAll=false",
    "test:coverage": "react-scripts test --coverage --env=jest-environment-jsdom-sixteen --watchAll=false",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "browser-test": "testcafe \"chrome --window-size='1920,1080'\" testcafe/e2e/",
    "browser-test:local": "BROWSER_TEST_ENV=local yarn browser-test",
    "browser-test:ci": "testcafe \"chrome:headless --disable-gpu --window-size='1920,1080'\" testcafe/e2e/",
    "codegen": "graphql-codegen -r dotenv/config dotenv_config_path=.env.development.local  --config codegen.yml",
    "storybook": "start-storybook -p 9009 -s public",
    "deploy-storybook": "storybook-to-ghpages",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__snapshots__/",
      "query.ts",
      "mutation.ts",
      "types.ts",
      "testUtils.ts",
      "/icons/",
      "serviceWorker.ts",
      "./src/index.tsx",
      "datepickerContext.tsx",
      "mockDataUtils.ts",
      "ScrollToTop.tsx",
      "scrollToTop.ts",
      "App.tsx",
      "apolloClient.ts",
      ".*stories.tsx",
      "graphql.tsx",
      "OidcCallback.tsx",
      "SilentRenew.tsx"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!hds-react)/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.4",
